    <item name="glide_tag" type="id"/>

    <declare-styleable name="DotView">
        <!-- 点的大小 -->
        <attr format="dimension" name="dotview_size"/>
        <!-- 线条的宽度 -->
        <attr format="dimension" name="dotview_line_size"/>
        <!-- 空心点的宽度 -->
        <attr format="dimension" name="dotview_hollow_width"/>
        <!-- 点的颜色 -->
        <attr format="color" name="dotview_color"/>
        <!-- 线的颜色 -->
        <attr format="color" name="dotview_color_line"/>
        <!--&lt;!&ndash; 线的颜色 &ndash;&gt;-->
        <!--<attr name="dotview_color_background" format="color"/>-->
        <!-- 点的类型 -->
        <attr name="dotview_type">
            <flag name="blank" value="0x0000"/>
            <flag name="fill" value="0x0001"/>
            <flag name="hollow" value="0x0002"/>
            <flag name="line_top" value="0x0004"/>
            <flag name="line_bottom" value="0x0008"/>
            <flag name="line_left" value="0x0010"/>
            <flag name="line_right" value="0x0020"/>
            <flag name="line_horizontal" value="0x0040"/>
            <flag name="line_vertical" value="0x0080"/>
        </attr>

    </declare-styleable>
    <declare-styleable name="RoundProgressbar">

        <!-- 圆环的宽度 -->
        <attr format="integer" name="ring_width"/>
        <!-- 进度[0,1]） -->
        <attr format="float" name="progress"/>
        <!-- 内容 -->
        <attr format="string" name="ring_text"/>

        <!-- 内容大小 -->
        <attr format="integer" name="ring_text_size"/>

        <!-- 圆的底色 -->
        <attr format="color" name="color_bg"/>
        <!-- 圆的进度颜色 -->
        <attr format="color" name="color_progress"/>
        <!-- 内容颜色 -->
        <attr format="color" name="color_text"/>

    </declare-styleable>
    <declare-styleable name="RoundTextView">

        <!-- 圆环的宽度 -->
        <attr format="integer" name="rtextview_width"/>
        <!-- 进度[0,1]） -->
        <attr format="float" name="rtextview_progress"/>

        <!-- 圆的底色 -->
        <attr format="color" name="rtextview_bg_color"/>
        <!-- 圆的进度颜色 -->
        <attr format="color" name="rtextview_progress_color"/>

        <!-- 是否显示圆环 -->
        <attr format="boolean" name="rtextview_show"/>

    </declare-styleable>
    <declare-styleable name="SidebarView2">
        <!-- 点击时的背景色 -->
        <attr format="color" name="sv2_bg_click_color"/>
        <!-- 文字的颜色 -->
        <attr format="color" name="sv2_text_color"/>
        <!-- 文字点击时的颜色 -->
        <attr format="color" name="sv2_text_click_color"/>
        <!-- 文字大小 -->
        <attr format="dimension" name="sv2_text_size"/>
        <!-- 用于设置分割单元的个数：默认为26 -->
        <attr format="integer" name="sv2_min_len"/>
    </declare-styleable>
    <declare-styleable name="StarBarView">
        <!--设置星星间的间隔-->
        <attr format="dimension" name="space_width"/>
        <!--星星间宽度-->
        <attr format="dimension" name="star_width"/>
        <!--星星间高度-->
        <attr format="dimension" name="star_height"/>
        <!--最大数量-->
        <attr format="integer" name="star_max"/>
        <!--选中数量-->
        <attr format="float" name="star_rating"/>
        <!--未选中图片-->
        <attr format="reference" name="star_hollow"/>
        <!--选中图片-->
        <attr format="reference" name="star_solid"/>
        <!--是否可以滑动改变选中数量-->
        <attr format="boolean" name="star_isIndicator"/>
        <!--排列方向-->
        <attr format="enum" name="star_orientation">
            <enum name="vertical" value="1"/>
            <enum name="horizontal" value="0"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="TRoundImageView">
        <attr format="dimension" name="radius"/>
        <attr format="dimension" name="left_top_radius"/>
        <attr format="dimension" name="right_top_radius"/>
        <attr format="dimension" name="right_bottom_radius"/>
        <attr format="dimension" name="left_bottom_radius"/>
    </declare-styleable>
